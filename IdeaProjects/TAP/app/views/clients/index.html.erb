<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Gender</th>
      <th>Blood type</th>
      <th>State</th>
      <th>Country</th>
      <th>Age range</th>
      <th>Zodiac</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Age</th>
      <th>Weight</th>
      <th>Height</th>
      <th>Occupation</th>
      <th>Premium</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.gender_id %></td>
        <td><%= client.blood_type_id %></td>
        <td><%= client.state_id %></td>
        <td><%= client.country_id %></td>
        <td><%= client.age_range_id %></td>
        <td><%= client.zodiac_id %></td>
        <td><%= client.first_name %></td>
        <td><%= client.last_name %></td>
        <td><%= client.age %></td>
        <td><%= client.weight %></td>
        <td><%= client.height %></td>
        <td><%= client.occupation %></td>
        <td><%= client.premium %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
